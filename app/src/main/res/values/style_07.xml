<?xml version="1.0" encoding="utf-8"?>

<resources>
    <style name="SettingRow">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/_48sdp</item>
        <item name="android:paddingStart">@dimen/_16sdp</item>
        <item name="android:paddingEnd">@dimen/_16sdp</item>
        <item name="android:background">#FFFFFF</item>
    </style>

    <style name="RowIcon">
        <item name="android:layout_width">@dimen/_20sdp</item>
        <item name="android:layout_height">@dimen/_20sdp</item>
    </style>

    <style name="RowTitle">
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="RowValue">
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:textColor">#6B6B6B</item>
    </style>

    <style name="ArrowIcon">
        <item name="android:layout_width">@dimen/_14sdp</item>
        <item name="android:layout_height">@dimen/_14sdp</item>
    </style>

</resources>